<% if @errors %>
    <% @errors.each do |error| %>
        <p><%= error %><p>
    <% end %>
    <% end %>
<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name  %><br>
<%= f.label :super_name %>
<%= f.text_field :super_name %><br>
<%= collection_select(:heroine, :power_id, Power.all, :id, :name) %>
<%= f.submit  %>
<% end %>